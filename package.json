{
	"name": "expensify-app",
	"version": "1.0.0",
	"main": "app.js",
	"license": "MIT",
	"scripts": {
		"serve": "live-server public/",
		"build:dev": "webpack",
		"build:prod": "webpack -p --env production",
		"dev-server": "webpack-dev-server",
		"start": "node server/server.js",
		"heroku-postbuild": "yarn run build:prod"
	},
	"eslintConfig": {
		"env": {
			"browser": true,
			"node": true
		},
		"rules": {
			"react/jsx-filename-extension": [
				1,
				{
					"extensions": [
						".js",
						".jsx"
					]
				}
			]
		}
	},
	"dependencies": {
		"babel-cli": "^6.26.0",
		"babel-core": "^6.26.0",
		"babel-loader": "^7.1.4",
		"babel-plugin-transform-class-properties": "^6.24.1",
		"babel-plugin-transform-object-rest-spread": "^6.26.0",
		"babel-preset-env": "1.5.2",
		"babel-preset-react": "6.24.1",
		"css-loader": "^0.28.11",
		"extract-text-webpack-plugin": "^3.0.2",
		"global": "^4.3.2",
		"live-server": "^1.2.0",
		"moment": "^2.22.2",
		"node-sass": "^4.8.3",
		"normalize.css": "^8.0.0",
		"react": "^16.3.2",
		"react-addons-shallow-compare": "15.6.0",
		"react-dates": "12.7.0",
		"react-dom": "^16.3.2",
		"react-modal": "^3.4.2",
		"react-redux": "^5.0.7",
		"react-router-dom": "^4.2.2",
		"react-with-styles": "^3.2.1",
		"redux": "^4.0.0",
		"sass-loader": "^7.0.1",
		"style-loader": "^0.21.0",
		"uuid": "^3.2.1",
		"validator": "^9.4.1",
		"webpack": "^4.5.0",
		"webpack-dev-server": "^3.1.3"
	},
	"devDependencies": {
		"webpack-cli": "^2.0.14"
	}
}
